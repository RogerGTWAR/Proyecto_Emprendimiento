  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "concurrently \"npm:dev:front\" \"npm:dev:back\"",
    "dev:front": "npm --prefix frontend run dev",
    "dev:back": "npm --prefix backend run dev",
    "build": "npm --prefix frontend run build && npm --prefix backend run build",
    "start": "npm --prefix backend run start",
    "start:front": "npm --prefix frontend run dev",
    "start:back": "npm --prefix backend run dev",
    "postinstall": "npm --prefix frontend install && npm --prefix backend install",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RogerGTWAR/Proyecto_Emprendimiento.git"
